{
  "name": "@sapmigr/xs-migration-cloud",
  "name-scope": "@sapmigr",
  "displayName": "XS Migration",
  "publisher": "C123123",
  "version": "1.1.47",
  "description": "SAP XS Migration Assistant",
  "calc-migration": "1.0.33",
  "jre": "81.50.50",
  "node": "10.15.2",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:xs-migration.start"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "xs-migration.start",
        "title": "SAP - XS Migration"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Xs_Migration_Logging",
      "properties": {
        "Xs_Migration_Logging.loggingLevel": {
          "type": "string",
          "enum": [
            "off",
            "fatal",
            "error",
            "warn",
            "info",
            "debug",
            "trace"
          ],
          "default": "trace",
          "description": "The verbosity of logging. The Order is Non < fatal < error < warn < info < debug < trace.",
          "scope": "window"
        },
        "Xs_Migration_Logging.maskDebugPassword": {
          "type": "boolean",
          "default": true,
          "description": "Should debug logs mask passwords?",
          "scope": "window"
        },
        "Xs_Migration_Logging.sourceLocationTracking": {
          "type": "boolean",
          "default": false,
          "description": "Should Source Code Location Info be added to log entries, DANGER - May be very slow, only use in debugging scenarios",
          "scope": "window"
        }
      }
    }
  },
  "scripts": {
    "build-ext": "node ./tools/ext/pre-install && npm run install-dev && vsce package && node ./tools/ext/post-install",
    "install-dev": "npm run download-manual-deps",
    "download-manual-deps": "grunt download-manual-deps",
    "move-manual-deps": "grunt move-manual-deps",
    "package": "grunt package-linuxx86_64 package-ntamd64",
    "test": "mocha -R xunit-file --exit",
    "test-jenkins": "mocha -R xunit-file --exit || true",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R xunit-file && istanbul report cobertura --root ./coverage --dir ./coverage",
    "coverage-jenkins": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R xunit-file || true && istanbul report cobertura --root ./coverage --dir ./coverage",
    "deploy-extension": "wex deploy -f $EXTENSION_FILE -e $HOST"
  },
  "repository": {
    "type": "git",
    "url": "git@github.wdf.sap.corp/XSA-Migration/xs-migration.git"
  },
  "extensionDependencies": [
    "sapos.yeoman-ui"
  ],
  "dependencies": {
    "@vscode-logging/types": "^0.1.3",
    "@vscode-logging/wrapper": "^1.0.1",
    "archiver": "3.0.0",
    "async": "2.6.0",
    "chalk-pipe": "^4.0.0",
    "cli-color": "1.2.0",
    "clone": "2.1.1",
    "datauri": "^3.0.0",
    "dotenv": "4.0.0",
    "enum": "2.5.0",
    "escodegen": "1.9.0",
    "esprima": "4.0.0",
    "estraverse": "4.2.0",
    "fs-extra": "4.0.2",
    "glob-to-regexp": "0.3.0",
    "hdb": "^0.15.0",
    "htmldom": "4.0.11",
    "inquirer": "^7.3.3",
    "is-valid-domain": "0.0.17",
    "js-yaml": "3.13.1",
    "jsonschema": "1.2.0",
    "lodash": "^4.17.20",
    "minimist": "1.2.5",
    "mkdirp": "0.5.1",
    "node-xml-compare": "^0.1.0",
    "path-is-absolute": "1.0.1",
    "readline-sync": "1.4.7",
    "underscore": "1.12.1",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "@sapse/business-application-studio-extension-management": "^0.6.3",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.52.0",
    "chai": "3.5.0",
    "chai-string": "1.3.0",
    "eslint": "^7.28.0",
    "expect": "1.20.2",
    "glob": "^7.1.6",
    "grunt": "^1.1.0",
    "grunt-contrib-compress": "1.4.1",
    "grunt-contrib-copy": "1.0.0",
    "grunt-curl": "^2.5.1",
    "grunt-move": "0.1.3",
    "grunt-zip": "0.17.1",
    "istanbul": "0.4.5",
    "madge": "0.6.0",
    "mocha": "9.0.0",
    "open": "7.0.3",
    "proxyquire": "1.7.11",
    "rewire": "2.5.2",
    "temp": "0.8.3",
    "uglify-js": "3.1.4",
    "unzip": "0.1.11",
    "vsce": "^1.63.0",
    "vscode-test": "^1.4.1",
    "xunit-file": "1.0.0"
  },
  "files": [
    "*.vsix"
  ]
}